{
    "version": 2,
    "builds": [
      {
        "src": "ecommerce/wsgi.py",
        "use": "@vercel/python",
        "config": { "maxLambdaSize": "15mb", "runtime": "python3.10" }
      },
      {
        "src": "build.sh",
        "use": "@vercel/static-build",
        "config": {
          "distDir": "ecommerce/staticfiles_build"
        }
      }
    ],
    "routes": [
      {
        "src": "/static/(.*)",
        "dest": "/static/$1"
      },
      {
        "src": "/(.*)",
        "dest": "ecommerce/wsgi.py"
      }
    ],

    "outputDirectory": "ecommerce/staticfiles_build"
}